block verticalMenuConfig

- var dashLink = "../../../html/"+ useDirection +"/"+ useLayout +"/"+ "index.html"

// main menu
.main-menu(class="#{menuType} #{menuColor} #{menuOpenType}", class=(menuNativeScroll == true ? 'menu-native-scroll' : ''), class=(menuIconRight == true ? 'menu-icon-right' : ''), class=(menuBorder == true ? 'menu-bordered' : ''), class=(menuShadow == true ? 'menu-shadow' : ''))

  // main menu header
  include ../includes/menu-header
  // / main menu header

  // main menu content
  .main-menu-content
    +verticalMenu([

      //- Dashbaord
      {url:dashLink,name:"Dashboard",slug:"dashboard",icon:"icon-home3", i18n:"nav.dash.main",tag:"5" , tagcustom: "tag tag-primary tag-pill float-xs-right mr-2"},

      //- Starter Kit start
      {url:"#", name:"Starter kit", slug:"", icon:"icon-bulb", i18n:"nav.starter_kit.main",
        //- Starter Kit Submenu
        submenu:[
          {url:"layout-1-column.html", name:"1 column", slug:"1-column", i18n:"nav.starter_kit.1_column"},
          {url:"layout-2-columns.html", name:"2 columns", slug:"2-columns", i18n:"nav.starter_kit.2_columns"},
          {url:"#", name:"Content Sidebar", slug:"", i18n:"nav.starter_kit.3_columns.main",
            submenu:[
              {url:"layout-content-left-sidebar.html", name:"Left sidebar", slug:"content-left-sidebar", i18n:"nav.starter_kit.3_columns.3_columns_left_sidebar"},
              {url:"layout-content-left-sticky-sidebar.html", name:"Left sticky sidebar", slug:"content-left-sticky-sidebar", i18n:"nav.starter_kit.3_columns.3_columns_left_sticky_sidebar"},
              {url:"layout-content-right-sidebar.html", name:"Right sidebar", slug:"content-right-sidebar", i18n:"nav.starter_kit.3_columns.3_columns_right_sidebar"},
              {url:"layout-content-right-sticky-sidebar.html", name:"Right sticky sidebar", slug:"content-right-sticky-sidebar", i18n:"nav.starter_kit.3_columns.3_columns_right_sticky_sidebar"}
            ]
          },
          {url:"#", name:"Content Det. Sidebar", slug:"", i18n:"nav.starter_kit.3_columns_detached.main",
            submenu:[
              {url:"layout-content-detached-left-sidebar.html", name:"Detached left sidebar", slug:"content-detached-left-sidebar", i18n:"nav.starter_kit.3_columns_detached.3_columns_detached_left_sidebar"},
              {url:"layout-content-detached-left-sticky-sidebar.html", name:"Detached sticky left sidebar", slug:"content-detached-left-sticky-sidebar", i18n:"nav.starter_kit.3_columns_detached.3_columns_detached_sticky_left_sidebar"},
              {url:"layout-content-detached-right-sidebar.html", name:"Detached right sidebar", slug:"content-detached-right-sidebar", i18n:"nav.starter_kit.3_columns_detached.3_columns_detached_right_sidebar"},
              {url:"layout-content-detached-right-sticky-sidebar.html", name:"Detached sticky right sidebar", slug:"content-detached-right-sticky-sidebar", i18n:"nav.starter_kit.3_columns_detached.3_columns_detached_sticky_right_sidebar"},
            ]
          },
          {divider:"1"},
          {url:"layout-fixed-navbar.html",name:"Fixed navbar",slug:"fixed-navbar", i18n:"nav.starter_kit.fixed_navbar"},
          {url:"layout-fixed-navigation.html",name:"Fixed navigation",slug:"fixed-navigation", i18n:"nav.starter_kit.fixed_navigation"},
          {url:"layout-fixed-navbar-navigation.html",name:"Fixed navbar & navigation",slug:"fixed-navbar-navigation", i18n:"nav.starter_kit.fixed_navbar_navigation"},
          {url:"layout-fixed-navbar-footer.html",name:"Fixed navbar & footer",slug:"fixed-navbar-footer", i18n:"nav.starter_kit.fixed_navbar_footer"},
          {divider:"1"},
          {url:"layout-fixed.html",name:"Fixed layout",slug:"fixed", i18n:"nav.starter_kit.fixed_layout"},
          {url:"layout-boxed.html",name:"Boxed layout",slug:"boxed", i18n:"nav.starter_kit.boxed_layout"},
          {url:"layout-static.html",name:"Static layout",slug:"static", i18n:"nav.starter_kit.static_layout"},
          {divider:"1"},
          {url:"layout-light.html",name:"Light layout",slug:"light", i18n:"nav.starter_kit.light_layout"},
          {url:"layout-dark.html",name:"Dark layout",slug:"dark", i18n:"nav.starter_kit.dark_layout"},
          {url:"layout-semi-dark.html",name:"Semi dark layout",slug:"semi-dark", i18n:"nav.starter_kit.semi_dark_layout"},
        ]
      },
      //- Starter Kit Ends

      //- Changelog
      {url:"changelog.html" ,name:"Changelog" ,icon:"icon-copy",slug:"changelog" ,tag:"1.2.2" , tagcustom: "tag tag-pill tag-danger float-xs-right", i18n:"nav.changelog.main"},

      //- Support
      {navheader:"Support",icon:"icon-ellipsis", i18n:"nav.category.support"},
        {url:"http://support.pixinvent.com/",name:"Raise Support",slug:"support-raise-support",icon:"icon-support", i18n:"nav.support_raise_support.main"},
        {url:"https://pixinvent.com/bootstrap-admin-template/robust/documentation",name:"Documentation",slug:"documentation",icon:"icon-document-text", i18n:"nav.support_documentation.main"},

    ])

  // /main menu content

  // main menu footer
  // include includes/menu-footer
  // main menu footer
// / main menu