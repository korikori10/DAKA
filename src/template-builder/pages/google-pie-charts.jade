block pageVars
	- var pageTitle    = "google Pie Chart"
	- var pageSubTitle = "google pie chart examples"
	- var description  = "A pie chart (or a donut chart) is a circular statistical graphic, which is divided into slices to illustrate numerical proportion. In a pie chart, the arc length of each slice (and consequently its central angle and area), is proportional to the quantity it represents."
	- var activeMenu   = "google-pie-charts"

extends template

append breadcrumbs
	+breadcrumbs([{url:"index.html",name:"Home"},{url:"#",name:"Google Charts"}, {name:"pie Chart"}])



//- Include page content in page block
append content
	include ../contents/google-pie-charts.html

//- Vendor JS
//------------------------------
//- Add vendor specific JS
append vendorjs
	script(src='https://www.google.com/jsapi', type='text/javascript')

//- Page JS
//------------------------------
//- Add custom page specific JS
append pagejs
	script(src='../../../app-assets/js/scripts/charts/google/pie/pie.js', type='text/javascript')
	script(src='../../../app-assets/js/scripts/charts/google/pie/pie-exploded.js', type='text/javascript')
	script(src='../../../app-assets/js/scripts/charts/google/pie/pie-rotated.js', type='text/javascript')
	script(src='../../../app-assets/js/scripts/charts/google/pie/3d-pie.js', type='text/javascript')
	script(src='../../../app-assets/js/scripts/charts/google/pie/3d-pie-exploded.js', type='text/javascript')
	script(src='../../../app-assets/js/scripts/charts/google/pie/donut.js', type='text/javascript')
	script(src='../../../app-assets/js/scripts/charts/google/pie/donut-exploded.js', type='text/javascript')
	script(src='../../../app-assets/js/scripts/charts/google/pie/donut-rotated.js', type='text/javascript')
	script(src='../../../app-assets/js/scripts/charts/google/pie/pie-diff-radius.js', type='text/javascript')
	script(src='../../../app-assets/js/scripts/charts/google/pie/pie-diff-border.js', type='text/javascript')
	script(src='../../../app-assets/js/scripts/charts/google/pie/pie-diff-opacity.js', type='text/javascript')
	script(src='../../../app-assets/js/scripts/charts/google/pie/pie-diff-invert.js', type='text/javascript')