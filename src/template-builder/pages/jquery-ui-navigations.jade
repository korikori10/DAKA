block pageVars
	- var pageTitle = "jQuery UI navigations"
	- var pageSubTitle = "jQuery UI navigations examples"
	- var description = "jQuery UI navigations include accordion, menu and tabs."
	- var activeMenu = "jquery-ui-navigations"

extends template

append breadcrumbs
	+breadcrumbs([{url:"index.html",name:"Home"},{url:"#",name:"jQuery UI"}, {name:"Navigations"}])



//- Include page content in page block
append content
	include ../contents/jquery-ui-navigations.html


//- Vendor CSS
//------------------------------
//- Add vendor specific CSS
append vendorcss
	link(rel='stylesheet', type='text/css', href='../../../app-assets/vendors/css/ui/jquery-ui.min.css')

//- Vendor JS
//------------------------------
//- Add vendor specific JS
append vendorjs
	script(src='../../../app-assets/js/core/libraries/jquery_ui/jquery-ui.min.js', type='text/javascript')

//- Page CSS
//------------------------------
//- Add page specific CSS
append pagecss
	link(rel='stylesheet', type='text/css', href='../../../app-assets/css#{rtl}/plugins/ui/jqueryui.css')

//- Page JS
//------------------------------
//- Add custom page specific JS
append pagejs
	script(src='../../../app-assets/js/scripts/ui/jquery-ui/navigations.js', type='text/javascript')