block pageVars
	- var pageTitle    = "Content right Sidebar"
	- var pageSubTitle = "content layout with right sidebar"
	- var description  = "The content layout page structure with navigation, content area and right sidebar."
	- var activeMenu   = "content-right-sidebar"

extends template

append pageConfig
	- var headerType = 'sk-header'
	- var contentLayout = 'content-right-sidebar'
	- var starterKit    = true

append breadcrumbs
	+breadcrumbs([{url:"index.html",name:"Home"},{url:"#",name:"Starters Kit"}, {name:"Content Left Sidebar"}])

//- include  page content in content black
append content
	include ../../contents/starter-kit/starter-page-content.html

append sidebar
	include ../../contents/starter-kit/sk-sidebar-common.html

