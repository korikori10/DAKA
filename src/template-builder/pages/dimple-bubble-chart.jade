block pageVars
	- var pageTitle    = "Dimple Bubble Chart"
	- var pageSubTitle = "Dimple bubble chart examples"
	- var description  = "A bubble chart is used to visualize a data set with two to four dimensions. The first two dimensions are visualized as coordinates, the third as color and the fourth as size."
	- var activeMenu   = "dimple-bubble-chart"

extends template

append breadcrumbs
	+breadcrumbs([{url:"/",name:"Home"},{url:"#",name:"Dimple Charts"}, {name:"Bubble Chart"}])

//- Include page content in page block
append content
	include ../contents/dimple-bubble-chart.html

//- Vendor JS
//------------------------------
//- Add vendor specific JS
append vendorjs
	script(src='../../../app-assets/vendors/js/charts/d3.min.js', type='text/javascript')
	script(src='../../../app-assets/vendors/js/charts/dimple.min.js', type='text/javascript')

//- Page JS
//------------------------------
//- Add custom page specific JS
append pagejs
	script(src='../../../app-assets/js/scripts/charts/dimple/bubble.js', type='text/javascript')