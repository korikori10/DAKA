block pageVars
	- var pageTitle    = "Long Press"
	- var pageSubTitle = "Easily write accented or rare characters"
	- var description  = "jQuery plugin to ease the writing of accented or rare characters."
	- var activeMenu   = "ex-component-long-press"

extends template

append breadcrumbs
	+breadcrumbs([{url:"index.html",name:"Home"},{url:"#",name:"Extra Components"}, {name:"Long Press"}])



//- Include page content in page block
append content
	include ../contents/ex-component-long-press.html

//- Vendor CSS
//------------------------------
//- Add vendor specific CSS
append vendorcss
	link(rel='stylesheet', type='text/css', href='../../../app-assets/vendors/css/extensions/long-press.css')

//- Vendor JS
//------------------------------
//- Add vendor specific JS
append vendorjs
	script(src='../../../app-assets/vendors/js/extensions/long-press/jquery.mousewheel.js', type='text/javascript')
	script(src='../../../app-assets/vendors/js/extensions/long-press/jquery.longpress.js', type='text/javascript')
	script(src='../../../app-assets/vendors/js/extensions/long-press/plugins.js', type='text/javascript')

//- Page JS
//------------------------------
//- Add custom page specific JS
append pagejs
	script(src='../../../app-assets/js/scripts/extensions/long-press.js', type='text/javascript')