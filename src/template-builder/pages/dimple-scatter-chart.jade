block pageVars
	- var pageTitle    = "Dimple Scatter Chart"
	- var pageSubTitle = "Dimple scatter chart examples"
	- var description  = "A scatter plot (also called a scatter graph, scatter chart, scattergram, or scatter diagram) is a type of plot or mathematical diagram using Cartesian coordinates to display values for typically two variables for a set of data. If the points are color-coded you can increase the number of displayed variables to three. The data is displayed as a collection of points, each having the value of one variable determining the position on the horizontal axis and the value of the other variable determining the position on the vertical axis."
	- var activeMenu   = "dimple-scatter-chart"

extends template

append breadcrumbs
	+breadcrumbs([{url:"index.html",name:"Home"},{url:"#",name:"Dimple Charts"}, {name:"Scatter Chart"}])

//- Include page content in page block
append content
	include ../contents/dimple-scatter-chart.html

//- Vendor JS
//------------------------------
//- Add vendor specific JS
append vendorjs
	script(src='../../../app-assets/vendors/js/charts/d3.min.js', type='text/javascript')
	script(src='../../../app-assets/vendors/js/charts/dimple.min.js', type='text/javascript')

//- Page JS
//------------------------------
//- Add custom page specific JS
append pagejs
	script(src='../../../app-assets/js/scripts/charts/dimple/scatter.js', type='text/javascript')